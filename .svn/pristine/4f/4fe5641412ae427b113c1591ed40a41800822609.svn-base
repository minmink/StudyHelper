package edu.skku.finalproject.studyhelper.model;

public class Member {
    String name = "";
    String att = "";
    Location location = null;

    public Location getLocation() {
        return location;
    }

    public void setLocation(Location location) {
        this.location = location;
    }

    public Member() {
    }

    public Member(String name, String att, Location location) {
        this.name = name;
        this.att = att;
        this.location = location;
    }

    public Member(String name, Location location) {
        this.name = name;
        this.location = location;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getAtt() {
        return att;
    }

    public void setAtt(String att) {
        this.att = att;
    }
}

